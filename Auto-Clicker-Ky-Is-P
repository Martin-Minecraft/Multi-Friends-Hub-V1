local Library = loadstring(game:HttpGet("https://pastebin.com/raw/2xDTKdpV", true))()

local AimbotTab = Library:CreateTab("AutoFarm", "This is where you modify the AutoFarm", true)
AimbotTab:CreateButton("Auto Click", function(arg)
click = false
m = game.Players.LocalPlayer:GetMouse()
m.KeyDown:connect(function(key)
if key == "p" then
if click == true then click = false
elseif
click == false then click = true

while click == true do 
wait(time)
mouse1click()
end
end
end
end)


end)
